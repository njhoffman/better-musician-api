{
  "extends" : "../.eslintrc",
  "env" : {
    "mocha" : true
  },
  "globals" : {
    "_initLogger_": false,
    "proxyquire":   false,
    "expect":       false,
    "request":      false,
    "should":       false,
    "sinon":        false,
    "jest":         false
  },
  "rules" : {
    "func-names"            : "off",
    "prefer-arrow-callback" : "off",
    "no-unused-expressions" : "off",
    "import/no-webpack-loader-syntax" : "off"
  }
}
