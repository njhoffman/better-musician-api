{
  "name": "instrumental-api",
  "version": "1.0.0",
  "description": "API for Instrumental App",
  "main": "index.js",
  "scripts": {
    "lint": "eslint bin config lib tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "codecov": "cat coverage/*/lcof.info | codecov",
    "deploy": "better-npm-run deploy"
  },
  "betterScripts": {
    "start": {
      "command": "node bin/server",
      "env": {
        "DEBUG": "api:*"
      }
    },
    "dev": {
      "command": "nodemon --config ./nodemon.json bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "api:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "DEBUG": "api:*",
        "NODE_ENV": "test"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run test && npm run clean && npm run compile",
      "env": {
        "DEBUG": "api:*"
      }
    }
  },
  "author": "Nick Hoffman",
  "license": "ISC",
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-register": "^6.18.0",
    "bcrypt": "^1.0.2",
    "better-npm-run": "0.0.13",
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.0",
    "express": "^4.14.0",
    "http": "0.0.0",
    "ip": "^1.1.4",
    "multer": "^1.2.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "piping": "^1.0.0-rc.4",
    "request": "^2.79.0",
    "rethinkdb": "^2.3.3",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "karma": "^1.3.0",
    "morgan": "^1.7.0",
    "nodemon": "^1.11.0",
    "plato": "^1.7.0",
    "prettyjson-256": "https://github.com/njhoffman/prettyjson-256/tarball/master"
  }
}
