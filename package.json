{
  "name": "better-musician-api",
  "version": "1.1.6",
  "description": "API for BetterMusician App",
  "main": "index.js",
  "scripts": {
    "postinstall": "./bin/init-hooks",
    "lint": "./node_modules/.bin/esw bin config lib test --color",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --cache --watch --clear --changed",
    "lint:watch:all": "npm run lint -- --watch --clear",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "deploy": "better-npm-run deploy",
    "test": "NODE_ENV=test mocha",
    "test:mochista": "NODE_ENV=test mochista --reporter spec --opts ./test/mochista.opts",
    "test:watch": "NODE_ENV=test mocha --watch --reporter spec",
    "coverage": "better-npm-run coverage",
    "coverage:watch": "npm run test:mochista -- --watch",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls && rm -rf coverage"
  },
  "betterScripts": {
    "start": {
      "command": "bin/rethinkdb.sh & node bin/server",
      "env": {
        "NODE_ENV": "production",
        "API_PORT": 4001,
        "DEBUG": "api:*"
      }
    },
    "dev": {
      "command": "nodemon --config ./nodemon.json bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "api:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run test && npm run clean && npm trigger production pull and build",
      "env": {
        "DEBUG": "api:*"
      }
    },
    "coverage": {
      "command": "nyc mocha",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "author": "Nick Hoffman",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "bcrypt": "^3.0.2",
    "better-npm-run": "^0.1.1",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "chalk": "^2.4.1",
    "compression": "^1.7.3",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "debug": "^4.1.0",
    "express": "^4.16.4",
    "geoip-lite": "^1.3.3",
    "http": "0.0.0",
    "json-pretty-html": "1.1.5",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "node-statsd": "^0.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "request": "^2.88.0",
    "response-time": "^2.3.2",
    "rethinkdb": "^2.3.3",
    "supertest": "^3.3.0",
    "useragent": "^2.3.0",
    "uuid": "^3.3.2",
    "validator": "^10.9.0",
    "yargs": "^12.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-watch": "^4.0.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mochista": "^0.17.0",
    "nodemon": "^1.18.6",
    "nyc": "^13.1.0",
    "plato": "^1.7.0",
    "proxyquire": "^2.1.0",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.2.0"
  }
}
